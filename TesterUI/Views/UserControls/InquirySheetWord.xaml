<UserControl x:Class="TesterUI.Views.UserControls.InquirySheetWord"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TesterUI.Views.UserControls"
             mc:Ignorable="d" Background="White"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TextBox Height="30" Width="100" Margin="5 10 0 0" HorizontalAlignment="Left"/>
        <TextBox Height="30" Width="100" Margin="5 10 0 0" HorizontalAlignment="Left"/>
        <TextBox Height="30" Width="100" Margin="5 10 0 0" HorizontalAlignment="Left"/>
        <TextBox Height="30" Width="100" Margin="5 10 0 0" HorizontalAlignment="Left"/>
        <TextBox Height="30" Width="100" Margin="5 10 0 0" HorizontalAlignment="Left"/>

        <Grid Margin="5 20 0 0">
            <StackPanel Orientation="Horizontal">
                <Button Content="A" Width="100" Height="40" Margin="5 0 0 0"/>
                <Button Content="B" Width="100" Height="40" Margin="5 0 0 0"/>
                <Button Content="C" Width="100" Height="40" Margin="5 0 0 0"/>
                <Button Content="D" Width="100" Height="40" Margin="5 0 0 0"/>
            </StackPanel>
        </Grid>
    </StackPanel>
</UserControl>
