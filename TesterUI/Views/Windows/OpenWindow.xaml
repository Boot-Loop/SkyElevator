<Window x:Class="TesterUI.Views.Windows.OpenWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TesterUI.Views.Windows"
        mc:Ignorable="d" WindowStyle="None"
        Title="OpenWindow" Height="450" Width="800">
    <Grid>
        <Button Height="20" Width="20" VerticalAlignment="Top" HorizontalAlignment="Right" Click="Button_Click">
            <Image Source="/Images/close.png"/>
        </Button>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="New Project" Height="40" Width="120"/>
            <Separator Height="20"/>
            <Button Content="Exist Project" Height="40" Width="120"/>
        </StackPanel>
    </Grid>
</Window>
